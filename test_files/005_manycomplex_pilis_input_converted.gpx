		<groundspeak:placed_by>Pilis50</groundspeak:placed_by>
		<groundspeak:owner id="14330">Pilis50 [06-209/555-296]</groundspeak:owner>
		<groundspeak:type>Traditional Cache</groundspeak:type>
		<groundspeak:difficulty>2</groundspeak:difficulty>
