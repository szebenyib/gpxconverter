	<groundspeak:cache id="1041" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Téry út</groundspeak:name>
		<groundspeak:placed_by>Geobébi és Gasp</groundspeak:placed_by>
		<groundspeak:owner id="1376">geobebi [+36302560511]</groundspeak:owner>
