	<type><![CDATA[Geocache]]></type>
	<groundspeak:cache id="640" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Nagy-Kevély</groundspeak:name>
		<groundspeak:placed_by>Anikó és Miki</groundspeak:placed_by>
		<groundspeak:owner id="28">MikiCache [+3620/383-5849]</groundspeak:owner>
		<groundspeak:type>Multi-cache</groundspeak:type>
		<groundspeak:difficulty>2</groundspeak:difficulty>
