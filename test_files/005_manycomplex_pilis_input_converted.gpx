	<groundspeak:cache id="348801" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Kisoroszi séta (-C)</groundspeak:name>
		<groundspeak:placed_by>Police107, Miley98</groundspeak:placed_by>
		<groundspeak:owner id="46343">police107 [+36-20-9653961]</groundspeak:owner>
