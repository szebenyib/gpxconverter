	<groundspeak:cache id="286901" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Kerékpárral az érsek városába</groundspeak:name>
		<groundspeak:placed_by>Blue Mali</groundspeak:placed_by>
		<groundspeak:owner id="34063">Blue Mali [06209441527]</groundspeak:owner>
