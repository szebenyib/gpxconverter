	<groundspeak:cache id="441801" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Séta Piliscsabán</groundspeak:name>
		<groundspeak:placed_by>KBB-Balin</groundspeak:placed_by>
		<groundspeak:owner id="78361">KBB-Balin [20-824-4502]</groundspeak:owner>
		<groundspeak:type>Multi-cache</groundspeak:type>
