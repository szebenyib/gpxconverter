	<groundspeak:cache id="4676" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Mária Valéria híd</groundspeak:name>
		<groundspeak:placed_by>Geogirl, Bagee</groundspeak:placed_by>
		<groundspeak:owner id="79604">GeoGirl [06703386443]</groundspeak:owner>
		<groundspeak:type>Traditional Cache</groundspeak:type>
		<groundspeak:difficulty>2</groundspeak:difficulty>
