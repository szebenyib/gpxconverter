	<groundspeak:cache id="4963" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Visegrádi-hegység, Lukács-árok</groundspeak:name>
		<groundspeak:placed_by>Blue Mali&amp;Piroskaa1</groundspeak:placed_by>
		<groundspeak:owner id="34063">Blue Mali [06209441527]</groundspeak:owner>
		<groundspeak:type>Traditional Cache</groundspeak:type>
		<groundspeak:difficulty>2.5</groundspeak:difficulty>
