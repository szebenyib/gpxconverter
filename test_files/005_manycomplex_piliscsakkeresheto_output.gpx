	<groundspeak:cache id="482501" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Visegrádi-hegység, kerékpáros kör 2</groundspeak:name>
		<groundspeak:placed_by>Blue Mali, Piroskaa1. Terepbejárást végezte: Griffs</groundspeak:placed_by>
		<groundspeak:owner id="34063">Blue Mali [06209441527]</groundspeak:owner>
