	<groundspeak:cache id="440" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Sas-szikla</groundspeak:name>
		<groundspeak:placed_by>Gálos András, Szepy, Kőhalmiék</groundspeak:placed_by>
		<groundspeak:owner id="534">GalosA [06-20/994-7141]</groundspeak:owner>
