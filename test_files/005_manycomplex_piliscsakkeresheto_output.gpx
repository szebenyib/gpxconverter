	<groundspeak:cache id="394" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Dömösi-szoros</groundspeak:name>
		<groundspeak:placed_by>Snake, Kokó</groundspeak:placed_by>
