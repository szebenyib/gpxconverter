	<groundspeak:cache id="3718" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Monalovaci rejtély</groundspeak:name>
		<groundspeak:placed_by>halaszkiraly és lizzard csapata</groundspeak:placed_by>
		<groundspeak:owner id="31957">halaszkiraly [három-0/626-511-egy]</groundspeak:owner>
