	<groundspeak:cache id="5004" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Hubertusz kunyhó</groundspeak:name>
		<groundspeak:placed_by>skiccpausz</groundspeak:placed_by>
