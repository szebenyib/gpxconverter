	<groundspeak:cache id="5011" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Ariadne fonala</groundspeak:name>
		<groundspeak:placed_by>skiccpausz</groundspeak:placed_by>
		<groundspeak:owner id="36159">skiccpausz [0036207704150]</groundspeak:owner>
