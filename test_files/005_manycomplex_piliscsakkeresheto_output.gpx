	<groundspeak:cache id="214" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Hosszú Hegy</groundspeak:name>
		<groundspeak:placed_by>ÁÉ,ÁS</groundspeak:placed_by>
		<groundspeak:owner id="281">ÉvaSanKri [+36 20 579 6081]</groundspeak:owner>
