	<groundspeak:cache id="3801" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Trézsi (Szeretet) - forrás</groundspeak:name>
		<groundspeak:placed_by>DonRazzino majd Pilis50</groundspeak:placed_by>
		<groundspeak:owner id="34063">Blue Mali [06209441527]</groundspeak:owner>
		<groundspeak:type>Traditional Cache</groundspeak:type>
		<groundspeak:difficulty>1.5</groundspeak:difficulty>
