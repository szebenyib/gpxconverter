	<groundspeak:cache id="1246" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Dunai hajós</groundspeak:name>
		<groundspeak:placed_by>B &amp; B</groundspeak:placed_by>
