	<groundspeak:cache id="3442" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Visegrádi kilátás</groundspeak:name>
		<groundspeak:placed_by>Gyombi (Kokó)</groundspeak:placed_by>
		<groundspeak:owner id="2826">Gyombi [20/828-9686]</groundspeak:owner>
		<groundspeak:type>Traditional Cache</groundspeak:type>
		<groundspeak:difficulty>2</groundspeak:difficulty>
