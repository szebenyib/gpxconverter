	<groundspeak:cache id="3200" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Váci Vártemplom</groundspeak:name>
		<groundspeak:placed_by>Kokó</groundspeak:placed_by>
