	<groundspeak:cache id="4668" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
		<groundspeak:name>Boldog Özséb-kilátó</groundspeak:name>
		<groundspeak:placed_by>MirP</groundspeak:placed_by>
		<groundspeak:owner id="26306">MirP [+36706279000]</groundspeak:owner>
		<groundspeak:type>Virtual Cache</groundspeak:type>
